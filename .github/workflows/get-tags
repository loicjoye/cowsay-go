jobs:
  get-tags:
    runs-on: ubuntu-latest
    steps:
      - name: Checkout Repository
        uses: actions/checkout@v4
        with:
          fetch-depth: 0  # Important pour récupérer tout l'historique
      - name: List All Tags
        id: tags
        run: |
          git fetch --tags
          echo "TAGS=$(git tag -l | tr '\n' ' ')" >> $GITHUB_OUTPUT